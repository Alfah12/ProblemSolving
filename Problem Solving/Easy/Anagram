function anagram(s: string): number {
    if (s.length % 2 !== 0) return -1

    let firstHalfWord = s.substring(0, s.length / 2),
        secoundHalfWord = s.substring(s.length / 2),
        counter = 0

    for (let i = 0; i < firstHalfWord.length; i++) {
        if (secoundHalfWord.indexOf(s[i]) != -1) {
            secoundHalfWord = secoundHalfWord.replace(s[i], '')
        }
        else {
            counter++
        }

    }

    return counter
}
