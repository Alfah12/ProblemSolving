      static string[] splitString(string s)
        {
            
            List<string> splitedString = new List<string>();
            string tempString = "";

            s += " ";
            for (int i = 0; i < s.Length; i++)
            {
                if (s[i] == ' ')
                {
                    splitedString.Add(tempString);
                    tempString = "";
                    continue;
                }

                else
                    tempString += s[i];
            }

            return splitedString.ToArray();
        } 
