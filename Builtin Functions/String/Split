static string[] splitString(string s, string splitAt)
{
    List<string> splitedString = new List<string>();
    string tempString = "";

    s += splitAt;

    for (int i = 0; i < s.Length; i++)
    {
        if (s[i].ToString() != splitAt)
        {
            tempString += s[i];
        }
        else
        {
            if (tempString != "")
                splitedString.Add(tempString);
            tempString = "";
        }
    }

    return splitedString.ToArray();
}
