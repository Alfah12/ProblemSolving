 static string toUpperCase(string s)
        {
            string result = "";
            for (int i = 0; i < s.Length; i++)
            {
                if ((s[i] >= 97 && s[i] <= 122))
                    result += (char)(((int)s[i]) - 32);
                else
                    result += s[i];
            }
            return result;
        }
