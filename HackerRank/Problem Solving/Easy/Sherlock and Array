
        public static string balancedSums(List<int> arr)
        {
            int sum = sumListElements(arr);
            int left = 0;

            foreach (int fu in arr)
            {
                if (2 * left == sum - fu)
                {
                    return "YES";
                }
                else
                {
                    left += fu;
                }
            }
            return "NO";
        }

        public static int sumListElements(List<int> lst)
        {
            int sum = 0;
            for (int i = 0; i < lst.Count; i++)
                sum += lst[i];

            return sum;
        }
