using System;
using System.Collections.Generic;

namespace lab
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine(isLowerCase("saleem kassab"));
        }
        static bool isUpperChar(char c) =>
            (int)c >= 65 && (int)c <= 90;
        static bool isLowerCase(string s)
        {
            for (int i = 0; i < s.Length; i++)
                if (isUpperChar(s[i]))
                    return false;

            return true;
        }
    }
}
