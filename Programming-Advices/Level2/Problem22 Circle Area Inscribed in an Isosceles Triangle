static void Main(string[] args)
        {
            Console.Write("Enter number1: ");
            double a = double.Parse(Console.ReadLine());
            Console.Write("Enter number1: ");
            double b = double.Parse(Console.ReadLine());

            Console.WriteLine(3.14 * ((b * b) / 4) * (2 * a - b) / (2 * a + b));
        }
