static string trimRight(string s)
{
    string result = "";
    int lastIndex = lastCharIndex(s);

    for (int i = 0; i <=lastIndex ; i++)
            result += s[i];

    return result;
}

static int lastCharIndex(string s)
{
    for (int i = s.Length - 1; i >= 0; i--)
        if (s[i] != ' ')
            return i;
    return -1;
}
