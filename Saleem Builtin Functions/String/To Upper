 static string ToUpper(string s)
        {
            string result = "";
            for (int i = 0; i < s.Length; i++)
                    result += charToUpperCase(s[i]);

            return result;
        }

static char ToUpper(char c) => (int)c >= 97 && (int)c <= 122 ? (char)((int)c - 32) : c;
