-- Create table
create table TENOR_DIMENSIONS
(
  id        NUMBER(10) generated always as identity,
  name      NVARCHAR2(200),
  tablename NVARCHAR2(200),
  pks       NVARCHAR2(200),
  fks       NVARCHAR2(200),
  subjectid NUMBER(10) not null,
  tenantid  NVARCHAR2(200),
  isdeleted NUMBER(1) not null
)
tablespace TECH_DATA_TS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate indexes 
create index IX_TENOR_DIMENSIONS_ISDELETED on TENOR_DIMENSIONS (ISDELETED)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index IX_TENOR_DIMENSIONS_SUBJECTID on TENOR_DIMENSIONS (SUBJECTID)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index IX_TENOR_DIMENSIONS_TENANTID on TENOR_DIMENSIONS (TENANTID)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate primary, unique and foreign key constraints 
alter table TENOR_DIMENSIONS
  add constraint PK_TENOR_DIMENSIONS primary key (ID)
  using index 
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
alter table TENOR_DIMENSIONS
  add constraint FK_TENOR_DIMENSIONS_SUBJECTID foreign key (SUBJECTID)
  references TENOR_SUBJECTS (ID) on delete cascade;
alter table TENOR_DIMENSIONS
  add constraint FK_TENOR_DIMENSIONS_TENANTID foreign key (TENANTID)
  references TENOR_TENANTS (ID);
