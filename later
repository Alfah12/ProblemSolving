-- Create table
create table TENOR_SUBJECTS
(
  id          NUMBER(10) generated always as identity,
  name        NVARCHAR2(200),
  description NVARCHAR2(400),
  mindateunit NUMBER(10) not null,
  tenantid    NVARCHAR2(200),
  isdeleted   NUMBER(1) not null
)
tablespace TECH_DATA_TS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate indexes 
create index IX_TENOR_SUBJECTS_ISDELETED on TENOR_SUBJECTS (ISDELETED)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index IX_TENOR_SUBJECTS_TENANTID on TENOR_SUBJECTS (TENANTID)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate primary, unique and foreign key constraints 
alter table TENOR_SUBJECTS
  add constraint PK_TENOR_SUBJECTS primary key (ID)
  using index 
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
alter table TENOR_SUBJECTS
  add constraint FK_TENOR_SUBJECTS_TENANTID foreign key (TENANTID)
  references TENOR_TENANTS (ID);
