-- Create table
create table TENOR_LEVELS
(
  id                NUMBER(10) generated always as identity,
  levelorder        NUMBER(10) not null,
  islist            NUMBER(1) not null,
  isdate            NUMBER(1) not null,
  canlevel          NUMBER(1) not null,
  canfilter         NUMBER(1) not null,
  islast            NUMBER(1) not null,
  orderby           NVARCHAR2(200),
  dimensionid       NUMBER(10) not null,
  isdeleted         NUMBER(1) not null,
  candynamic        NUMBER(1) not null,
  name              NVARCHAR2(200),
  columnname        NVARCHAR2(200),
  dynamicstring     NVARCHAR2(200),
  helpercolumn      NVARCHAR2(200),
  helperconcatstart NVARCHAR2(200),
  helperconcatend   NVARCHAR2(200)
)
tablespace TECH_DATA_TS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate indexes 
create index IX_TENOR_LEVELS_DIMENSIONID on TENOR_LEVELS (DIMENSIONID)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index IX_TENOR_LEVELS_ISDELETED on TENOR_LEVELS (ISDELETED)
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate primary, unique and foreign key constraints 
alter table TENOR_LEVELS
  add constraint PK_TENOR_LEVELS primary key (ID)
  using index 
  tablespace TECH_DATA_TS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
alter table TENOR_LEVELS
  add constraint FK_TENOR_LEVELS_DIMENSIONID foreign key (DIMENSIONID)
  references TENOR_DIMENSIONS (ID) on delete cascade;
