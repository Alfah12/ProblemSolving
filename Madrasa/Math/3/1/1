static void printYourOld(byte num)
        {
            // 1 year = |
            // 10 years = #
            byte tenYears = (byte)(num / 10),
            reminderYears = (byte)(num % 10);
            for (byte i = 0; i < tenYears; i++)
                Console.Write('#');
            for (byte i = 0; i < reminderYears; i++)
                Console.Write('|');
        }
