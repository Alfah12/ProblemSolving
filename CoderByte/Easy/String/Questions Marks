using System;

class MainClass {

  public static string QuestionsMarks(string str) {

       int tempNumber = 0, digitCounter = 0, questionMarksCounter = 0;
            bool hasCondition = false;

            for (int i = 0; i < str.Length; i++)
            {
                if (Char.IsDigit(str[i]))
                {
                    digitCounter++;
                    tempNumber += int.Parse(str[i].ToString());
                }

                if (tempNumber > 0 && str[i] == '?')
                    questionMarksCounter++;

                if (tempNumber == 10)
                {
                    if (questionMarksCounter == 3)
                    {
                        hasCondition = true;
                        tempNumber = int.Parse(str[i].ToString());
                        questionMarksCounter = 0;
                        digitCounter = 0;
                    }
                    else
                    {
                        return "false";
                    }
                }
            }

            return hasCondition? "true" : "false";

  }

  static void Main() {  
    // keep this function call here
    Console.WriteLine(QuestionsMarks(Console.ReadLine()));
  } 

}
